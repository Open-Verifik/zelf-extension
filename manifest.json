{
	"manifest_version": 3,
	"name": "Verifik Wallet",
	"version": "1.0",
	"permissions": ["activeTab", "storage"],
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "/app/favicon.ico",
			"48": "/app/favicon.ico",
			"128": "/app/favicon8icog"
		}
	},
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "/app/favicon.ico",
		"48": "/app/favicon.ico",
		"128": "/app/favicon8icog"
	}
}
